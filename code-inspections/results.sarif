      "results": [
        {
          "ruleId": "MIT0017S",
          "ruleIndex": 0,
          "level": "warning",
          "message": {
            "text": "Unfiltered data is being sent to the client, which might result in sensitive data being exposed."
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "index.js",
                  "uriBaseId": "%SRCROOT%"
