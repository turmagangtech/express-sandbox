      ],
      "rules": [
        {
          "id": "MIT0017S",
          "shortDescription": {
            "text": "Avoid sending unfiltered data to the client."
          },
          "fullDescription": {
            "text": "Filter the user data before sending it to the client, and avoid sending sensitive data such as hashed passwords or private keys."
          },
          "help": {
            "text": "Update the query to select only the columns needed. For example: db.select('id', 'name', 'email').from('users')"
          },
          "defaultConfiguration": {
            "level": "warning"
          }
        }
      ]
    }
  ]
